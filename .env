# AI API Configuration - Dual Provider Setup
# Add your actual API keys here

# OpenAI API Key (Primary - ChatGPT GPT-4)
# Get your key from: https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=sk-your-actual-openai-key-here

# Google Gemini API Key (Fallback)
# Get your key from: https://aistudio.google.com/app/apikey  
REACT_APP_GEMINI_API_KEY=your-actual-gemini-key-here

# Provider Configuration
REACT_APP_PRIMARY_AI_PROVIDER=openai
REACT_APP_FALLBACK_AI_PROVIDER=gemini

# API Configuration
REACT_APP_MAX_RETRIES=3
REACT_APP_REQUEST_TIMEOUT=30000
REACT_APP_ENABLE_FAILOVER=true

# Debug Mode
REACT_APP_DEBUG_AI=true

# Pro Editor Integration Feature Flag
VITE_PRO_EDITOR_INTEGRATION=false
REACT_APP_PRO_EDITOR_INTEGRATION=false
